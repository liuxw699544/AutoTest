[
  {
    "description": "这是携带headers信息访问的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
         "json": {
           "name": "wanglaosi",
           "sex": "woman"

         }
      },
    "response": {
      "json": {
        "wanglaosi": "success",
        "status": "1"
      }
    }
    }
  ]