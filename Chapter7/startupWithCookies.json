[
  {
    "description": "这是会返回cookies信息的get请求",
    "request": {
      "uri": "/getcookies",
      "method": "get"
    },
    "response": {
      "cookies":{
        "login": "true"
      },
      "text": "获取cookies成功",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description": "这是携带cookies信息访问的get请求",
    "request": {
      "uri": "/get/with/cookies",
      "method": "get",
      "cookies":{
        "login": "true"
      }
    },
    "response": {
      "text": "携带cookies信息才能访问的get请求"
      }
    },
  {"description": "这是携带cookies信息才能访问的post请求",
    "request": {
      "uri": "/post/with/cookies",
      "method": "post",
      "cookies":{
        "login": "true"

      },
      "json":{
        "name": "huhansan",
        "age": "18"
        }
    },
    "response": {
      "status": 200,
      "json": {
        "huhansan": "success",
        "status": "1"
        }
      }
    }
  ]